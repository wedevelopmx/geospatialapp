<!DOCTYPE html>
<html>
  <head>
    <title><%= title %></title>
    <link rel='stylesheet' href='/css/style.css' />
    <link rel='stylesheet' href='/css/main.css' />
    <link href="vendor/fontawesome/css/font-awesome.min.css" rel="stylesheet">

    <link href="vendor/nouislider/jquery.nouislider.css" rel="stylesheet" />
    <link href="vendor/openlayers3/build/ol.css" rel="stylesheet" />
    <link href="vendor/angular-openlayers-directive/dist/angular-openlayers-directive.css" rel="stylesheet">
    
  </head>
  <body ng-app="geospatial" ng-controller="MainController as mainCtrl">
  	<header id="header" data-ng-include="'template/header.html'"></header>

  	<section id="main">
	  	<aside id="sidebar" data-ng-include="'template/sidebar.html'"></aside>
        <section class="notification" ng-class="{ active: message }" >
            <span ng-class="{danger: message.type == 'danger'}" ng-bind="message.text"></span>
        </section>
	  	<section ng-view></section>
	</section>

    <script type="text/javascript" src="vendor/angular/angular.js"></script>
    <script type="text/javascript" src="vendor/angular-route/angular-route.js"></script>
    <script type="text/javascript" src="vendor/angular-resource/angular-resource.js"></script>
    <script type="text/javascript" src="vendor/angular-sanitize/angular-sanitize.js"></script>
    <script type="text/javascript" src="vendor/ngstorage/ngStorage.js"></script>
    <script type="text/javascript" src="vendor/jquery/dist/jquery.js"></script>
    
    <!-- Third Party-->    
    <script type="text/javascript" src="vendor/underscore/underscore.js"></script>
    <script type="text/javascript" src="vendor/openlayers3/build/ol.js"></script>
    <script type="text/javascript" src="vendor/angular-openlayers-directive/dist/angular-openlayers-directive.js"></script>
    <script type="text/javascript" src="vendor/angular-underscore/angular-underscore.js"></script>
    <script type="text/javascript" src="vendor/angular-bootstrap/ui-bootstrap-tpls.js"></script>
    <script type="text/javascript" src="vendor/angular-bootstrap/ui-bootstrap.js"></script>
    <script type="text/javascript" src="vendor/nouislider/Link.js"></script>
    <script type="text/javascript" src="vendor/nouislider/jquery.nouislider.js"></script>
    <script type="text/javascript" src="vendor/angular-nouislider/src/nouislider.min.js"></script>
    <script type="text/javascript" src="vendor/flot/jquery.flot.js"></script>
    <script type="text/javascript" src="vendor/flot/jquery.flot.pie.js"></script>

    <!-- Module: Geospatial -->
    <script type="text/javascript" src="js/app.js"></script>
    <!-- Controllers -->
    <script type="text/javascript" src="js/controllers/main-controller.js"></script>
    <script type="text/javascript" src="js/controllers/dashboard-controller.js"></script>
    <script type="text/javascript" src="js/controllers/project-controller.js"></script>
    <script type="text/javascript" src="js/controllers/team-controller.js"></script>
    <script type="text/javascript" src="js/controllers/survey-controller.js"></script>
    <script type="text/javascript" src="js/controllers/project-survey-controller.js"></script>
    <!-- Services -->
    <script type="text/javascript" src="js/services/project-service.js"></script>
    <script type="text/javascript" src="js/services/team-service.js"></script>
    <script type="text/javascript" src="js/services/survey-service.js"></script>
    <script type="text/javascript" src="js/services/section-service.js"></script>
    <script type="text/javascript" src="js/services/question-service.js"></script>
    <script type="text/javascript" src="js/services/answer-service.js"></script>
    <script type="text/javascript" src="js/services/map-service.js"></script>
    <script type="text/javascript" src="js/services/template-service.js"></script>
    <script type="text/javascript" src="js/services/auth-service.js"></script>
    <script type="text/javascript" src="js/services/country-service.js"></script>
    <script type="text/javascript" src="js/services/indicator-service.js"></script>
    <!-- Directives -->
    <script type="text/javascript" src="js/directives/selection-map.js"></script>
    <script type="text/javascript" src="js/directives/single-marker-map.js"></script>
    <script type="text/javascript" src="js/directives/markers-map.js"></script>
    <script type="text/javascript" src="js/directives/project-summary.js"></script>
    <script type="text/javascript" src="js/directives/auth-directive.js"></script>
    <script type="text/javascript" src="js/directives/survey/answer-directive.js"></script>
    <script type="text/javascript" src="js/directives/survey/question-directive.js"></script>
    <script type="text/javascript" src="js/directives/survey/section-directive.js"></script>
    <script type="text/javascript" src="js/directives/card/survey-card.js"></script>
    <script type="text/javascript" src="js/directives/card/section-card.js"></script>
    <script type="text/javascript" src="js/directives/card/question-card.js"></script>
  </body>
</html>
